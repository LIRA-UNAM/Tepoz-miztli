    </model>
    <!-- Aros -->
    <include> 
      <uri> ./my_window.sdf</uri> <name>window_1</name> <pose> -2 3 0.25 0 0 0</pose>
    </include>

    <include> 
      <uri> ./my_window.sdf</uri> <name>window_2</name> <pose> -1 3 0.25 0 0 0</pose>
    </include>

    <include> 
      <uri> ./my_window.sdf</uri> <name>window_3</name> <pose> 0 3 0.25 0 0 0</pose>
    </include>

    <include> 
      <uri> ./my_window.sdf</uri> <name>window_4</name> <pose> 1 3 0.25 0 0 0</pose>
    </include>

    <!--Pizarron-->
    <model name="whiteboard">
      <pose> 3 -4.9 1.25 0 0 1.5708</pose>
        <link name="whiteboard_link">
            <inertial>
                <mass>2.0</mass>
            </inertial>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.05 1.5 1.2</size>
                    </box>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <box>
                        <size>0.05 1.5 1.2</size>
                    </box>
              </geometry>

              <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
