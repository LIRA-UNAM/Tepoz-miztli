    <!--Plataforma Inicio -->
    <model name="platform start">
        <pose> -4 4 0.025 0 1.5708 0</pose>
        <link name="platform_link">
            <inertial>
                <mass>3.0</mass>
            </inertial>
            <collision name="collision">
                <geometry>
                    <box>
                        <size>0.05 1 1</size>
                    </box>
                </geometry>
            </collision>
            <visual name="visual">
                <geometry>
                    <box>
                        <size>0.05 1 1</size>
                    </box>
                </geometry>

                <material>
                    <ambient>1 1 1 1</ambient>
                    <diffuse>1 1 1 1</diffuse>
