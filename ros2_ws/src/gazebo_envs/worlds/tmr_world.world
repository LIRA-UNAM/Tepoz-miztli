    </model>

    <!--Plataforma Final -->
    <model name="platform end">
        <pose> -4 -4 0.025 0 1.5708 0</pose>
        <link name="platform_link">
            <inertial>
                <mass>3.0</mass>
            </inertial>
            <collision name="collision">
                <geometry>
